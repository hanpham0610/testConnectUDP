     const date = new Date(timestamp);\r\n        return date.toLocaleString('en-US', {\r\n          year: 'numeric',\r\n          month: '2-digit',\r\n          day: '2-digit',\r\n          hour: '2-digit',\r\n          minute: '2-digit',\r\n          second: '2-digit',\r\n          hour12: false,\r\n        });\r\n      },\r\n      updateChart(message) {\r\n    const values = message.split('-').map(Number);\r\n    const now = new Date();\r\n    const minutes = Math.floor(now.getMinutes());\r\n\r\n    // Kiểm tra xem biểu đồ đã sẵn sàng chưa\r\n    if (!this.chart) {\r\n      console.error(\"Biểu đồ chưa sẵn sàng.\");\r\n      return;\r\n    }\r\n\r\n    // Kiểm tra xem label (minutes) có tồn tại không\r\n    const index = this.chart.data.labels.indexOf(minutes);\r\n    if (index === -1) {\r\n      this.chart.data.labels.push(minutes);\r\n      this.chart.data.datasets.forEach((dataset, i) => {\r\n        dataset.data.push(values[i] || 0);\r\n      });\r\n    } else {\r\n      this.chart.data.datasets.forEach((dataset, i) => {\r\n        dataset.data[index] = values[i] || 0;\r\n      });\r\n    }\r\n\r\n    // Cập nhật biểu đồ\r\n    this.chart.update();\r\n  }\r\n\r\n    }\r\n  });\r\n  </script>\r\n  \r\n  <style>\r\n  .popup {\r\n    position: fixed;\r\n    top: 20%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    background-color: white;\r\n    padding: 20px;\r\n    border: 1px solid black;\r\n    z-index: 1000;\r\n  }\r\n  \r\n  .popup button {\r\n    margin-right: 10px;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAsCE,SAASA,eAAc,QAAS,KAAI;AACpC,SAASC,IAAG,QAAS,aAAY;AACjC,SAASC,KAAI,IAAKC,OAAO,EAAEC,KAAK,EAAEC,OAAO,EAAEC,MAAM,EAAEC,WAAW,EAAEC,YAAY,EAAEC,aAAa,EAAEC,WAAU,QAAS,UAAS;;AAEzH;AACAP,OAAO,CAACQ,QAAQ,CAACP,KAAK,EAAEC,OAAO,EAAEC,MAAM,EAAEC,WAAW,EAAEC,YAAY,EAAEC,aAAa,EAAEC,WAAW;AAE9F,eAAeV,eAAe,CAAC;EAC7BY,UAAU,EAAE;IACVC,SAAS,EAAEZ;EACb,CAAC;EACDa,IAAIA,CAAA,EAAG;IACT,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,KAAK;MA