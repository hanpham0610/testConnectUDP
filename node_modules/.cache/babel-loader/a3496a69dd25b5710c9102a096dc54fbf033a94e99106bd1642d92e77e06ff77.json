{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport LineChartVue from './LineChart.vue';\nexport default {\n  components: {\n    LineChartVue\n  },\n  data() {\n    return {\n      messages: [],\n      acceptedIps: [],\n      // Mảng chứa các IP đã được chấp nhận\n      showPopup: false,\n      currentIp: '',\n      // IP hiện tại đang cần xác nhận\n      pendingData: null,\n      // Dữ liệu đang chờ xử lý nếu người dùng chấp nhận\n      chartData: null,\n      chartOptions: null\n    };\n  },\n  mounted() {\n    this.chartData = this.$refs.lineChart.chartData;\n    this.chartOptions = this.$refs.lineChart.chartOptions;\n    const ws = new WebSocket('ws://localhost:8081');\n    ws.onmessage = event => {\n      const data = JSON.parse(event.data);\n      // Kiểm tra xem IP này đã được chấp nhận chưa\n      if (!this.acceptedIps.includes(data.ip)) {\n        this.currentIp = data.ip;\n        this.pendingData = data;\n        this.showPopup = true; // Hiển thị popup nếu chưa chấp nhận IP\n        this.$refs.lineChart.addData(data.message); // Thêm dữ liệu vào biểu đồ\n      } else {\n        // Nếu IP đã được chấp nhận, thêm dữ liệu vào danh sách\n        this.messages.push(data);\n      }\n    };\n  },\n  methods: {\n    acceptData() {\n      this.acceptedIps.push(this.currentIp); // Thêm IP vào danh sách đã chấp nhận\n      this.messages.push(this.pendingData); // Thêm dữ liệu vào danh sách\n      this.clearPopup();\n    },\n    rejectData() {\n      this.clearPopup(); // Chỉ ẩn popup mà không thêm dữ liệu\n    },\n    clearPopup() {\n      this.showPopup = false;\n      this.currentIp = '';\n      this.pendingData = null;\n    },\n    formattedTimestamp(timestamp) {\n      const date = new Date(timestamp);\n      return date.toLocaleString('en-US', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit',\n        hour12: false\n      });\n    }\n  }\n};","map":{"version":3,"names":["LineChartVue","components","data","messages","acceptedIps","showPopup","currentIp","pendingData","chartData","chartOptions","mounted","$refs","lineChart","ws","WebSocket","onmessage","event","JSON","parse","includes","ip","addData","message","push","methods","acceptData","clearPopup","rejectData","formattedTimestamp","timestamp","date","Date","toLocaleString","year","month","day","hour","minute","second","hour12"],"sources":["D:\\FE-VM\\democline\\src\\components\\testConnect.vue"],"sourcesContent":["<template>\r\n    <div class=\"container-fluid\"> \r\n        <div class=\"row\"> \r\n        <div class=\"col-12 col-md-3\">  \r\n      \r\n    <div id=\"app\">\r\n      <table class=\"table table-striped table-hover\">\r\n        <thead>\r\n            <td ><strong>IP</strong></td>\r\n            <td ><strong>Data</strong></td>\r\n            <td ><strong>Time</strong></td>\r\n  </thead>\r\n        <tbody>\r\n    <tr v-for=\"(item, index) in messages\" :key=\"index\">\r\n      <td >{{ item.ip }}</td>\r\n      <td>{{ item.message }}</td>\r\n      <td>{{ formattedTimestamp(item.timestamp) }}  </td>\r\n    </tr>\r\n  </tbody>\r\n</table>\r\n    </div>\r\n</div>   \r\n</div>\r\n<div class=\"col-12 col-md-9\">    \r\n      <LineChartVue :chart-data=\"chartData\" :options=\"chartOptions\"></LineChartVue>\r\n\r\n    </div>\r\n</div>\r\n     <!-- Popup confirmation dialog -->\r\n     <div v-if=\"showPopup\" class=\"popup\">\r\n      <p>Bạn có muốn nhận dữ liệu từ IP {{ currentIp }}?</p>\r\n      <button @click=\"acceptData\">Đồng ý</button>\r\n      <button @click=\"rejectData\">Từ chối</button>\r\n    </div>\r\n   \r\n  </template>\r\n  \r\n  <script>\r\nimport LineChartVue from './LineChart.vue';\r\nexport default {\r\n    components: {\r\n        LineChartVue\r\n  },\r\n  data() {\r\n    return {\r\n      messages: [],\r\n      acceptedIps: [], // Mảng chứa các IP đã được chấp nhận\r\n      showPopup: false,\r\n      currentIp: '', // IP hiện tại đang cần xác nhận\r\n      pendingData: null, // Dữ liệu đang chờ xử lý nếu người dùng chấp nhận\r\n      chartData: null,\r\n      chartOptions: null\r\n    };\r\n  },\r\n  mounted() {\r\n    this.chartData = this.$refs.lineChart.chartData;\r\n    this.chartOptions = this.$refs.lineChart.chartOptions;\r\n    const ws = new WebSocket('ws://localhost:8081');\r\n    ws.onmessage = (event) => {\r\n      const data = JSON.parse(event.data);\r\n      // Kiểm tra xem IP này đã được chấp nhận chưa\r\n      if (!this.acceptedIps.includes(data.ip)) {\r\n        this.currentIp = data.ip;\r\n        this.pendingData = data;\r\n        this.showPopup = true; // Hiển thị popup nếu chưa chấp nhận IP\r\n        this.$refs.lineChart.addData(data.message); // Thêm dữ liệu vào biểu đồ\r\n\r\n      } else {\r\n        // Nếu IP đã được chấp nhận, thêm dữ liệu vào danh sách\r\n        this.messages.push(data);\r\n      }\r\n    };\r\n  \r\n\r\n   \r\n  },\r\n\r\n  methods: {\r\n    acceptData() {\r\n      this.acceptedIps.push(this.currentIp); // Thêm IP vào danh sách đã chấp nhận\r\n      this.messages.push(this.pendingData); // Thêm dữ liệu vào danh sách\r\n      this.clearPopup();\r\n    },\r\n    rejectData() {\r\n      this.clearPopup(); // Chỉ ẩn popup mà không thêm dữ liệu\r\n    },\r\n    clearPopup() {\r\n      this.showPopup = false;\r\n      this.currentIp = '';\r\n      this.pendingData = null;\r\n    },\r\n    formattedTimestamp(timestamp) {\r\n      const date = new Date(timestamp);\r\n      return date.toLocaleString('en-US', {\r\n        year: 'numeric',\r\n        month: '2-digit',\r\n        day: '2-digit',\r\n        hour: '2-digit',\r\n        minute: '2-digit',\r\n        second: '2-digit',\r\n        hour12: false,\r\n      });\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.popup {\r\n  position: fixed;\r\n  top: 20%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  background-color: white;\r\n  padding: 20px;\r\n  border: 1px solid black;\r\n  z-index: 1000;\r\n}\r\n\r\n.popup button {\r\n  margin-right: 10px;\r\n}\r\n</style>\r\n  "],"mappings":";AAsCA,OAAOA,YAAW,MAAO,iBAAiB;AAC1C,eAAe;EACXC,UAAU,EAAE;IACRD;EACN,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MAAE;MACjBC,SAAS,EAAE,KAAK;MAChBC,SAAS,EAAE,EAAE;MAAE;MACfC,WAAW,EAAE,IAAI;MAAE;MACnBC,SAAS,EAAE,IAAI;MACfC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACF,SAAQ,GAAI,IAAI,CAACG,KAAK,CAACC,SAAS,CAACJ,SAAS;IAC/C,IAAI,CAACC,YAAW,GAAI,IAAI,CAACE,KAAK,CAACC,SAAS,CAACH,YAAY;IACrD,MAAMI,EAAC,GAAI,IAAIC,SAAS,CAAC,qBAAqB,CAAC;IAC/CD,EAAE,CAACE,SAAQ,GAAKC,KAAK,IAAK;MACxB,MAAMd,IAAG,GAAIe,IAAI,CAACC,KAAK,CAACF,KAAK,CAACd,IAAI,CAAC;MACnC;MACA,IAAI,CAAC,IAAI,CAACE,WAAW,CAACe,QAAQ,CAACjB,IAAI,CAACkB,EAAE,CAAC,EAAE;QACvC,IAAI,CAACd,SAAQ,GAAIJ,IAAI,CAACkB,EAAE;QACxB,IAAI,CAACb,WAAU,GAAIL,IAAI;QACvB,IAAI,CAACG,SAAQ,GAAI,IAAI,EAAE;QACvB,IAAI,CAACM,KAAK,CAACC,SAAS,CAACS,OAAO,CAACnB,IAAI,CAACoB,OAAO,CAAC,EAAE;MAE9C,OAAO;QACL;QACA,IAAI,CAACnB,QAAQ,CAACoB,IAAI,CAACrB,IAAI,CAAC;MAC1B;IACF,CAAC;EAIH,CAAC;EAEDsB,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACrB,WAAW,CAACmB,IAAI,CAAC,IAAI,CAACjB,SAAS,CAAC,EAAE;MACvC,IAAI,CAACH,QAAQ,CAACoB,IAAI,CAAC,IAAI,CAAChB,WAAW,CAAC,EAAE;MACtC,IAAI,CAACmB,UAAU,CAAC,CAAC;IACnB,CAAC;IACDC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACD,UAAU,CAAC,CAAC,EAAE;IACrB,CAAC;IACDA,UAAUA,CAAA,EAAG;MACX,IAAI,CAACrB,SAAQ,GAAI,KAAK;MACtB,IAAI,CAACC,SAAQ,GAAI,EAAE;MACnB,IAAI,CAACC,WAAU,GAAI,IAAI;IACzB,CAAC;IACDqB,kBAAkBA,CAACC,SAAS,EAAE;MAC5B,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,SAAS,CAAC;MAChC,OAAOC,IAAI,CAACE,cAAc,CAAC,OAAO,EAAE;QAClCC,IAAI,EAAE,SAAS;QACfC,KAAK,EAAE,SAAS;QAChBC,GAAG,EAAE,SAAS;QACdC,IAAI,EAAE,SAAS;QACfC,MAAM,EAAE,SAAS;QACjBC,MAAM,EAAE,SAAS;QACjBC,MAAM,EAAE;MACV,CAAC,CAAC;IACJ;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}