{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { Line } from 'vue-chartjs';\nimport { Chart as ChartJS, Title, Tooltip, Legend, LineElement, CategoryScale, LinearScale, PointElement } from 'chart.js';\nChartJS.register(Title, Tooltip, Legend, LineElement, CategoryScale, LinearScale, PointElement);\nexport default {\n  components: {\n    LineChart: {\n      extends: Line,\n      props: ['chartData', 'options'],\n      methods: {\n        addData(data) {\n          const values = data.split('-').map(Number);\n          this.chartData.labels.push(`${this.chartData.labels.length + 1} phút`);\n          values.forEach((value, index) => {\n            this.chartData.datasets[index].data.push(value);\n          });\n          if (this.chartData.labels.length > 30) {\n            this.chartData.labels.shift();\n            this.chartData.datasets.forEach(dataset => dataset.data.shift());\n          }\n          if (this.$data._chart) {\n            this.$data._chart.update(); // Cập nhật biểu đồ\n          }\n        }\n      }\n    }\n  },\n  data() {\n    return {\n      messages: [],\n      acceptedIps: [],\n      showPopup: false,\n      currentIp: '',\n      pendingData: null,\n      chartData: {\n        labels: [],\n        datasets: [{\n          label: 'Param 1',\n          backgroundColor: 'rgba(75, 192, 192, 0.2)',\n          borderColor: 'rgba(75, 192, 192, 1)',\n          data: []\n        }, {\n          label: 'Param 2',\n          backgroundColor: 'rgba(54, 162, 235, 0.2)',\n          borderColor: 'rgba(54, 162, 235, 1)',\n          data: []\n        }, {\n          label: 'Param 3',\n          backgroundColor: 'rgba(255, 206, 86, 0.2)',\n          borderColor: 'rgba(255, 206, 86, 1)',\n          data: []\n        }, {\n          label: 'Param 4',\n          backgroundColor: 'rgba(153, 102, 255, 0.2)',\n          borderColor: 'rgba(153, 102, 255, 1)',\n          data: []\n        }]\n      },\n      chartOptions: {\n        responsive: true,\n        scales: {\n          x: {\n            title: {\n              display: true,\n              text: 'Thời gian (phút)'\n            }\n          },\n          y: {\n            title: {\n              display: true,\n              text: 'Giá trị tham số'\n            },\n            min: 0,\n            max: 100\n          }\n        }\n      }\n    };\n  },\n  mounted() {\n    const ws = new WebSocket('ws://localhost:8081');\n    ws.onmessage = event => {\n      const data = JSON.parse(event.data);\n      if (!this.acceptedIps.includes(data.ip)) {\n        this.currentIp = data.ip;\n        this.pendingData = data;\n        this.showPopup = true;\n      } else {\n        this.messages.push(data);\n\n        // Đợi cho đến khi biểu đồ đã được render\n        this.$nextTick(() => {\n          this.$refs.lineChart.addData(data.message);\n        });\n      }\n    };\n  },\n  methods: {\n    acceptData() {\n      this.acceptedIps.push(this.currentIp);\n      this.messages.push(this.pendingData);\n\n      // Thêm dữ liệu vào biểu đồ khi người dùng chấp nhận\n      this.$nextTick(() => {\n        this.$refs.lineChart.addData(this.pendingData.message);\n      });\n      this.clearPopup();\n    },\n    rejectData() {\n      this.clearPopup();\n    },\n    clearPopup() {\n      this.showPopup = false;\n      this.currentIp = '';\n      this.pendingData = null;\n    },\n    formattedTimestamp(timestamp) {\n      const date = new Date(timestamp);\n      return date.toLocaleString('en-US', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit',\n        hour12: false\n      });\n    }\n  }\n};","map":{"version":3,"names":["Line","Chart","ChartJS","Title","Tooltip","Legend","LineElement","CategoryScale","LinearScale","PointElement","register","components","LineChart","extends","props","methods","addData","data","values","split","map","Number","chartData","