{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      messages: [],\n      acceptedIps: [],\n      // Mảng chứa các IP đã được chấp nhận\n      showPopup: false,\n      currentIp: '',\n      // IP hiện tại đang cần xác nhận\n      pendingData: null // Dữ liệu đang chờ xử lý nếu người dùng chấp nhận\n    };\n  },\n  mounted() {\n    const ws = new WebSocket('ws://localhost:8081');\n    ws.onmessage = event => {\n      const data = JSON.parse(event.data);\n      // Kiểm tra xem IP này đã được chấp nhận chưa\n      if (!this.acceptedIps.includes(data.ip)) {\n        this.currentIp = data.ip;\n        this.pendingData = data;\n        this.showPopup = true; // Hiển thị popup nếu chưa chấp nhận IP\n      } else {\n        // Nếu IP đã được chấp nhận, thêm dữ liệu vào danh sách\n        this.messages.push(data);\n      }\n    };\n  },\n  methods: {\n    acceptData() {\n      this.acceptedIps.push(this.currentIp); // Thêm IP vào danh sách đã chấp nhận\n      this.messages.push(this.pendingData); // Thêm dữ liệu vào danh sách\n      this.clearPopup();\n    },\n    rejectData() {\n      this.clearPopup(); // Chỉ ẩn popup mà không thêm dữ liệu\n    },\n    clearPopup() {\n      this.showPopup = false;\n      this.currentIp = '';\n      this.pendingData = null;\n    },\n    formattedTimestamp(timestamp) {\n      const date = new Date(timestamp);\n      return date.toLocaleString('en-US', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit',\n        fractionalSecondDigits: 3,\n        // For milliseconds\n        hour12: false,\n        timeZoneName: 'short'\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","messages","acceptedIps","showPopup","currentIp","pendingData","mounted","ws","WebSocket","onmessage","event","JSON","parse","includes","ip","push","methods","acceptData","clearPopup","rejectData","formattedTimestamp","timestamp","date","Date","toLocaleString","year","month","day","hour","minute","second","fractionalSecondDigits","hour12","timeZoneName"],"sources":["D:\\FE-VM\\democline\\src\\components\\testConnect.vue"],"sourcesContent":["<template>\r\n    <div id=\"app\">\r\n     \r\n      <table class=\"table table-striped table-hover\">\r\n        <thead>\r\n            <td ><strong>IP</strong></td>\r\n            <td ><strong>Data</strong></td>\r\n            <td ><strong>Time</strong></td>\r\n  </thead>\r\n        <tbody>\r\n    <tr v-for=\"(item, index) in messages\" :key=\"index\">\r\n      <td >{{ item.ip }}</td>\r\n      <td>{{ item.message }}</td>\r\n      <td>{{ formattedTimestamp(item.timestamp) }}  </td>\r\n    </tr>\r\n  </tbody>\r\n</table>\r\n    </div>\r\n\r\n     <!-- Popup confirmation dialog -->\r\n     <div v-if=\"showPopup\" class=\"popup\">\r\n      <p>Bạn có muốn nhận dữ liệu từ IP {{ currentIp }}?</p>\r\n      <button @click=\"acceptData\">Đồng ý</button>\r\n      <button @click=\"rejectData\">Từ chối</button>\r\n    </div>\r\n   \r\n  </template>\r\n  \r\n  <script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      messages: [],\r\n      acceptedIps: [], // Mảng chứa các IP đã được chấp nhận\r\n      showPopup: false,\r\n      currentIp: '', // IP hiện tại đang cần xác nhận\r\n      pendingData: null, // Dữ liệu đang chờ xử lý nếu người dùng chấp nhận\r\n    };\r\n  },\r\n  mounted() {\r\n    const ws = new WebSocket('ws://localhost:8081');\r\n    ws.onmessage = (event) => {\r\n      const data = JSON.parse(event.data);\r\n      // Kiểm tra xem IP này đã được chấp nhận chưa\r\n      if (!this.acceptedIps.includes(data.ip)) {\r\n        this.currentIp = data.ip;\r\n        this.pendingData = data;\r\n        this.showPopup = true; // Hiển thị popup nếu chưa chấp nhận IP\r\n      } else {\r\n        // Nếu IP đã được chấp nhận, thêm dữ liệu vào danh sách\r\n        this.messages.push(data);\r\n      }\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    acceptData() {\r\n      this.acceptedIps.push(this.currentIp); // Thêm IP vào danh sách đã chấp nhận\r\n      this.messages.push(this.pendingData); // Thêm dữ liệu vào danh sách\r\n      this.clearPopup();\r\n    },\r\n    rejectData() {\r\n      this.clearPopup(); // Chỉ ẩn popup mà không thêm dữ liệu\r\n    },\r\n    clearPopup() {\r\n      this.showPopup = false;\r\n      this.currentIp = '';\r\n      this.pendingData = null;\r\n    },\r\n    formattedTimestamp(timestamp) {\r\n      const date = new Date(timestamp);\r\n      return date.toLocaleString('en-US', {\r\n        year: 'numeric',\r\n        month: '2-digit',\r\n        day: '2-digit',\r\n        hour: '2-digit',\r\n        minute: '2-digit',\r\n        second: '2-digit',\r\n        fractionalSecondDigits: 3, // For milliseconds\r\n        hour12: false,\r\n        timeZoneName: 'short'\r\n      });\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.popup {\r\n  position: fixed;\r\n  top: 20%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  background-color: white;\r\n  padding: 20px;\r\n  border: 1px solid black;\r\n  z-index: 1000;\r\n}\r\n\r\n.popup button {\r\n  margin-right: 10px;\r\n}\r\n</style>\r\n  "],"mappings":";AA6BA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MAAE;MACjBC,SAAS,EAAE,KAAK;MAChBC,SAAS,EAAE,EAAE;MAAE;MACfC,WAAW,EAAE,IAAI,CAAE;IACrB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,EAAC,GAAI,IAAIC,SAAS,CAAC,qBAAqB,CAAC;IAC/CD,EAAE,CAACE,SAAQ,GAAKC,KAAK,IAAK;MACxB,MAAMV,IAAG,GAAIW,IAAI,CAACC,KAAK,CAACF,KAAK,CAACV,IAAI,CAAC;MACnC;MACA,IAAI,CAAC,IAAI,CAACE,WAAW,CAACW,QAAQ,CAACb,IAAI,CAACc,EAAE,CAAC,EAAE;QACvC,IAAI,CAACV,SAAQ,GAAIJ,IAAI,CAACc,EAAE;QACxB,IAAI,CAACT,WAAU,GAAIL,IAAI;QACvB,IAAI,CAACG,SAAQ,GAAI,IAAI,EAAE;MACzB,OAAO;QACL;QACA,IAAI,CAACF,QAAQ,CAACc,IAAI,CAACf,IAAI,CAAC;MAC1B;IACF,CAAC;EACH,CAAC;EAEDgB,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACf,WAAW,CAACa,IAAI,CAAC,IAAI,CAACX,SAAS,CAAC,EAAE;MACvC,