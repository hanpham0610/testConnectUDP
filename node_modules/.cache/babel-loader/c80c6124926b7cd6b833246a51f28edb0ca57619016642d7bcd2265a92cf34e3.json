{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { defineComponent } from 'vue';\nimport { Line } from 'vue-chartjs';\nimport { Chart as ChartJS, Title, Tooltip, Legend, LineElement, PointElement, CategoryScale, LinearScale } from 'chart.js';\n\n// Đăng ký các phần tử và thành phần cần thiết\nChartJS.register(Title, Tooltip, Legend, LineElement, PointElement, CategoryScale, LinearScale);\nexport default defineComponent({\n  components: {\n    LineChart: Line\n  },\n  data() {\n    return {\n      messages: [],\n      acceptedIps: [],\n      showPopup: false,\n      currentIp: '',\n      pendingData: null\n    };\n  },\n  mounted() {\n    const ws = new WebSocket('ws://localhost:8081');\n    ws.onmessage = event => {\n      const data = JSON.parse(event.data);\n      if (!this.acceptedIps.includes(data.ip)) {\n        this.currentIp = data.ip;\n        this.pendingData = data;\n        this.showPopup = true;\n      } else {\n        this.messages.push(data);\n\n        // Gọi updateChart sau khi đảm bảo biểu đồ đã sẵn sàng\n        this.$nextTick(() => {\n          this.updateChart(data.message);\n        });\n      }\n    };\n    this.$nextTick(() => {\n      this.chart = this.$refs.lineChart.chart;\n      if (this.chart) {\n        this.chart.data.labels = []; // Khởi tạo labels rỗng\n        this.chart.data.datasets = [{\n          label: 'Data 1',\n          data: [],\n          borderColor: 'blue',\n          fill: false\n        }, {\n          label: 'Data 2',\n          data: [],\n          borderColor: 'red',\n          fill: false\n        }, {\n          label: 'Data 3',\n          data: [],\n          borderColor: 'green',\n          fill: false\n        }, {\n          label: 'Data 4',\n          data: [],\n          borderColor: 'black',\n          fill: false\n        }];\n        this.chart.update(); // Cập nhật biểu đồ lần đầu\n      }\n    });\n  },\n  methods: {\n    acceptData() {\n      this.acceptedIps.push(this.currentIp);\n      this.messages.push(this.pendingData);\n      this.updateChart(this.pendingData.message);\n      this.clearPopup();\n    },\n    rejectData() {\n      this.clearPopup();\n    },\n    clearPopup() {\n      this.showPopup = false;\n      this.currentIp = '';\n      this.pendingData = null;\n    },\n    formattedTimestamp(timestamp) {\n      const date = new Date(timestamp);\n      return date.toLocaleString('en-US', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit',\n        hour12: false\n      });\n    },\n    updateChart(message) {\n      const values = message.split('-').map(Number);\n      const now = new Date();\n      const minutes = Math.floor(now.getMinutes());\n      if (!this.chart) {\n        console.error(\"Biểu đồ chưa sẵn sàng.\");\n        return;\n      }\n      const index = this.chart.data.labels.indexOf(minutes);\n      if (index === -1) {\n        this.chart.data.labels.push(minutes);\n        this.chart.data.datasets.forEach((dataset, i) => {\n          dataset.data.push(values[i] || 0);\n        });\n      } else {\n        this.chart.data.datasets.forEach((dataset, i) => {\n          dataset.data[index] = values[i] || 0;\n        });\n      }\n      this.chart.update(); // Cập nhật biểu đồ\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","Line","Chart","ChartJS","Title","Tooltip","Legend","LineElement","PointElement","CategoryScale","LinearScale","register","components","LineChart","data","messages","acceptedIps","showPopup","currentIp","pendingData","mounted","ws","WebSocket","onmessage","event","JSON","parse","includes","ip","push","$nextTick","updateChart","message","chart","$refs","lineChart","labels","datasets","label","borderColor","fill","update","methods","acceptData","clearPopup","rejectData","formattedTimestamp","timestamp","date","Date","toLocaleString","year","month","day","hour","minute","second","hour12","values","split","map","Number","now","minutes","Math","floor","getMinutes","console","error","index","indexOf","forEach","dataset","i"],"sources":["D:\\FE-VM\\democline\\src\\components\\testConnect.vue"],"sourcesContent":["<template>\r\n    <div class=\"container-fluid\">\r\n      <div class=\"row\">\r\n        <div class=\"col-12 col-md-3\">\r\n          <div id=\"app\">\r\n            <table class=\"table table-striped table-hover\">\r\n              <thead>\r\n                <tr>\r\n                  <th><strong>IP</strong></th>\r\n                  <th><strong>Data</strong></th>\r\n                  <th><strong>Time</strong></th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"(item, index) in messages\" :key=\"index\">\r\n                  <td>{{ item.ip }}</td>\r\n                  <td>{{ item.message }}</td>\r\n                  <td>{{ formattedTimestamp(item.timestamp) }}</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-12 col-md-9\">\r\n            <line-chart ref=\"lineChart\" :data=\"chartData\" :options=\"chartOptions\"></line-chart>\r\n        </div>\r\n      </div>\r\n  \r\n      <!-- Popup confirmation dialog -->\r\n      <div v-if=\"showPopup\" class=\"popup\">\r\n        <p>Bạn có muốn nhận dữ liệu từ IP {{ currentIp }}?</p>\r\n        <button @click=\"acceptData\">Đồng ý</button>\r\n        <button @click=\"rejectData\">Từ chối</button>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { defineComponent } from 'vue'\r\n  import { Line } from 'vue-chartjs'\r\n  import { Chart as ChartJS, Title, Tooltip, Legend, LineElement, PointElement, CategoryScale, LinearScale } from 'chart.js'\r\n\r\n  // Đăng ký các phần tử và thành phần cần thiết\r\n  ChartJS.register(Title, Tooltip, Legend, LineElement, PointElement, CategoryScale, LinearScale)\r\n  \r\n  export default defineComponent({\r\n  components: {\r\n    LineChart: Line\r\n  },\r\n  data() {\r\n    return {\r\n      messages: [],\r\n      acceptedIps: [],\r\n      showPopup: false,\r\n      currentIp: '',\r\n      pendingData: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    const ws = new WebSocket('ws://localhost:8081');\r\n    ws.onmessage = (event) => {\r\n      const data = JSON.parse(event.data);\r\n      if (!this.acceptedIps.includes(data.ip)) {\r\n        this.currentIp = data.ip;\r\n        this.pendingData = data;\r\n        this.showPopup = true;\r\n      } else {\r\n        this.messages.push(data);\r\n\r\n        // Gọi updateChart sau khi đảm bảo biểu đồ đã sẵn sàng\r\n        this.$nextTick(() => {\r\n          this.updateChart(data.message);\r\n        });\r\n      }\r\n    };\r\n\r\n    this.$nextTick(() => {\r\n      this.chart = this.$refs.lineChart.chart;\r\n      if (this.chart) {\r\n        this.chart.data.labels = []; // Khởi tạo labels rỗng\r\n        this.chart.data.datasets = [\r\n          { label: 'Data 1', data: [], borderColor: 'blue', fill: false },\r\n          { label: 'Data 2', data: [], borderColor: 'red', fill: false },\r\n          { label: 'Data 3', data: [], borderColor: 'green', fill: false },\r\n          { label: 'Data 4', data: [], borderColor: 'black', fill: false },\r\n        ];\r\n        this.chart.update(); // Cập nhật biểu đồ lần đầu\r\n      }\r\n    });\r\n  },\r\n  methods: {\r\n    acceptData() {\r\n      this.acceptedIps.push(this.currentIp);\r\n      this.messages.push(this.pendingData);\r\n      this.updateChart(this.pendingData.message);\r\n      this.clearPopup();\r\n    },\r\n    rejectData() {\r\n      this.clearPopup();\r\n    },\r\n    clearPopup() {\r\n      this.showPopup = false;\r\n      this.currentIp = '';\r\n      this.pendingData = null;\r\n    },\r\n    formattedTimestamp(timestamp) {\r\n      const date = new Date(timestamp);\r\n      return date.toLocaleString('en-US', {\r\n        year: 'numeric',\r\n        month: '2-digit',\r\n        day: '2-digit',\r\n        hour: '2-digit',\r\n        minute: '2-digit',\r\n        second: '2-digit',\r\n        hour12: false,\r\n      });\r\n    },\r\n    updateChart(message) {\r\n      const values = message.split('-').map(Number);\r\n      const now = new Date();\r\n      const minutes = Math.floor(now.getMinutes());\r\n\r\n      if (!this.chart) {\r\n        console.error(\"Biểu đồ chưa sẵn sàng.\");\r\n        return;\r\n      }\r\n\r\n      const index = this.chart.data.labels.indexOf(minutes);\r\n      if (index === -1) {\r\n        this.chart.data.labels.push(minutes);\r\n        this.chart.data.datasets.forEach((dataset, i) => {\r\n          dataset.data.push(values[i] || 0);\r\n        });\r\n      } else {\r\n        this.chart.data.datasets.forEach((dataset, i) => {\r\n          dataset.data[index] = values[i] || 0;\r\n        });\r\n      }\r\n\r\n      this.chart.update(); // Cập nhật biểu đồ\r\n    }\r\n  }\r\n});\r\n\r\n  </script>\r\n  \r\n  <style>\r\n  .popup {\r\n    position: fixed;\r\n    top: 20%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    background-color: white;\r\n    padding: 20px;\r\n    border: 1px solid black;\r\n    z-index: 1000;\r\n  }\r\n  \r\n  .popup button {\r\n    margin-right: 10px;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAsCE,SAASA,eAAc,QAAS,KAAI;AACpC,SAASC,IAAG,QAAS,aAAY;AACjC,SAASC,KAAI,IAAKC,OAAO,EAAEC,KAAK,EAAEC,OAAO,EAAEC,MAAM,EAAEC,WAAW,EAAEC,YAAY,EAAEC,aAAa,EAAEC,WAAU,QAAS,UAAS;;AAEzH;AACAP,OAAO,CAACQ,QAAQ,CAACP,KAAK,EAAEC,OAAO,EAAEC,MAAM,EAAEC,WAAW,EAAEC,YAAY,EAAEC,aAAa,EAAEC,WAAW;AAE9F,eAAeV,eAAe,CAAC;EAC/BY,UAAU,EAAE;IACVC,SAAS,EAAEZ;EACb,CAAC;EACDa,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,KAAK;MAChBC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,EAAC,GAAI,IAAIC,SAAS,CAAC,qBAAqB,CAAC;IAC/CD,EAAE,CAACE,SAAQ,GAAKC,KAAK,IAAK;MACxB,MAAMV,IAAG,GAAIW,IAAI,CAACC,KAAK,CAA