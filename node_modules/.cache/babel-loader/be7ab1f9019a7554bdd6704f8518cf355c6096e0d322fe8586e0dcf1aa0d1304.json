{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { defineComponent } from 'vue';\nimport { Line } from 'vue-chartjs';\nimport { Chart as ChartJS, Title, Tooltip, Legend, LineElement, PointElement, CategoryScale, LinearScale } from 'chart.js';\n\n// Đăng ký các phần tử và thành phần cần thiết\nChartJS.register(Title, Tooltip, Legend, LineElement, PointElement, CategoryScale, LinearScale);\nexport default defineComponent({\n  components: {\n    LineChart: Line\n  },\n  data() {\n    return {\n      messages: [],\n      acceptedIps: [],\n      showPopup: false,\n      currentIp: '',\n      pendingData: null,\n      chartData: {\n        labels: [],\n        // Minutes\n        datasets: [{\n          label: 'Parameter 1',\n          data: [],\n          borderColor: 'blue',\n          backgroundColor: 'rgba(0, 0, 255, 0.2)',\n          fill: false\n        }, {\n          label: 'Parameter 2',\n          data: [],\n          borderColor: 'red',\n          backgroundColor: 'rgba(255, 0, 0, 0.2)',\n          fill: false\n        }, {\n          label: 'Parameter 2',\n          data: [],\n          borderColor: 'red',\n          backgroundColor: 'rgba(255, 0, 0, 0.2)',\n          fill: false\n        }, {\n          label: 'Parameter 2',\n          data: [],\n          borderColor: 'red',\n          backgroundColor: 'rgba(255, 0, 0, 0.2)',\n          fill: false\n        }\n        // Add more datasets as needed\n        ]\n      },\n      chartOptions: {\n        responsive: true,\n        scales: {\n          x: {\n            title: {\n              display: true,\n              text: 'Minutes'\n            }\n          },\n          y: {\n            title: {\n              display: true,\n              text: 'Value'\n            },\n            min: 0,\n            max: 100\n          }\n        }\n      }\n    };\n  },\n  mounted() {\n    const ws = new WebSocket('ws://localhost:8081');\n    ws.onmessage = event => {\n      const data = JSON.parse(event.data);\n      if (!this.acceptedIps.includes(data.ip)) {\n        this.currentIp = data.ip;\n        this.pendingData = data;\n        this.showPopup = true;\n      } else {\n        this.messages.push(data);\n        this.updateChart(data.message);\n      }\n    };\n  },\n  methods: {\n    acceptData() {\n      this.acceptedIps.push(this.currentIp);\n      this.messages.push(this.pendingData);\n      this.updateChart(this.pendingData.message);\n      this.clearPopup();\n    },\n    rejectData() {\n      this.clearPopup();\n    },\n    clearPopup() {\n      this.showPopup = false;\n      this.currentIp = '';\n      this.pendingData = null;\n    },\n    formattedTimestamp(timestamp) {\n      const date = new Date(timestamp);\n      return date.toLocaleString('en-US', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit',\n        hour12: false\n      });\n    },\n    updateChart(message) {\n      const values = message.split('-').map(Number);\n      const now = new Date();\n      const minutes = Math.floor(now.getMinutes() / 1); // Round to nearest minute\n\n      const index = this.chartData.labels.indexOf(minutes);\n      if (index === -1) {\n        this.chartData.labels.push(minutes);\n        this.chartData.datasets.forEach((dataset, i) => {\n          dataset.data.push(values[i] || 0);\n        });\n      } else {\n        this.chartData.datasets.forEach((dataset, i) => {\n          dataset.data[index] = values[i] || 0;\n        });\n      }\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","Line","Chart","ChartJS","Title","Tooltip","Legend","LineElement","PointElement","CategoryScale","LinearScale","register","components","LineChart","data","messages","acceptedIps","showPopup","currentIp","pendingData","chartData","labels","datasets","label","borderColor","backgroundColor","fill","chartOptions","responsive","scales","x","title","