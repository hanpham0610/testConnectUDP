tip, Legend, LineElement, PointElement, CategoryScale, LinearScale } from 'chart.js'\r\n  \r\n  // Đăng ký các phần tử và thành phần cần thiết\r\n  ChartJS.register(Title, Tooltip, Legend, LineElement, PointElement, CategoryScale, LinearScale)\r\n  \r\n  export default defineComponent({\r\n    components: {\r\n      LineChart: Line\r\n    },\r\n    data() {\r\n      return {\r\n        messages: [],\r\n        acceptedIps: [],\r\n        showPopup: false,\r\n        currentIp: '',\r\n        pendingData: null,\r\n        chartData: {\r\n          labels: [], // Minutes\r\n          datasets: [\r\n            {\r\n              label: 'Parameter 1',\r\n              data: [],\r\n              borderColor: 'blue',\r\n              backgroundColor: 'rgba(0, 0, 255, 0.2)',\r\n              fill: false,\r\n            },\r\n            {\r\n              label: 'Parameter 2',\r\n              data: [],\r\n              borderColor: 'red',\r\n              backgroundColor: 'rgba(255, 0, 0, 0.2)',\r\n              fill: false,\r\n            },\r\n            {\r\n              label: 'Parameter 2',\r\n              data: [],\r\n              borderColor: 'red',\r\n              backgroundColor: 'rgba(255, 0, 0, 0.2)',\r\n              fill: false,\r\n            },\r\n            {\r\n              label: 'Parameter 2',\r\n              data: [],\r\n              borderColor: 'red',\r\n              backgroundColor: 'rgba(255, 0, 0, 0.2)',\r\n              fill: false,\r\n            },\r\n            // Add more datasets as needed\r\n          ]\r\n        },\r\n        chartOptions: {\r\n          responsive: true,\r\n          scales: {\r\n            x: {\r\n              title: {\r\n                display: true,\r\n                text: 'Minutes'\r\n              }\r\n            },\r\n            y: {\r\n              title: {\r\n                display: true,\r\n                text: 'Value'\r\n              },\r\n              min: 0,\r\n              max: 100\r\n            }\r\n          }\r\n        }\r\n      };\r\n    },\r\n    mounted() {\r\n      const ws = new WebSocket('ws://localhost:8081');\r\n      ws.onmessage = (event) => {\r\n        const data = JSON.parse(event.data);\r\n        if (!this.acceptedIps.includes(data.ip)) {\r\n          this.currentIp = data.ip;\r\n          this.pendingData = data;\r\n          this.showPopup = true;\r\n        } else {\r\n          this.messages.push(data);\r\n          this.updateChart(data.message);\r\n        }\r\n      };\r\n    },\r\n    methods: {\r\n      acceptData() {\r\n        this.acceptedIps.push(this.currentIp);\r\n        this.messages.push(this.pendingData);\r\n        this.updateChart(this.pendingData.message);\r\n        this.clearPopup();\r\n      },\r\n      rejectData() {\r\n        this.clearPopup();\r\n      },\r\n      clearPopup() {\r\n        this.showPopup = false;\r\n        this.currentIp = '';\r\n        this.pendingData = null;\r\n      },\r\n      formattedTimestamp(timestamp) {\r\n        const date = new Date(timestamp);\r\n        return date.toLocaleString('en-US', {\r\n          year: 'numeric',\r\n          month: '2-digit',\r\n          day: '2-digit',\r\n          hour: '2-digit',\r\n          minute: '2-digit',\r\n          second: '2-digit',\r\n          hour12: false,\r\n        });\r\n      },\r\n      updateChart(message) {\r\n        const values = message.split('-').map(Number);\r\n        const now = new Date();\r\n        const minutes = Math.floor(now.getMinutes() / 1); // Round to nearest minute\r\n  \r\n        const index = this.chartData.labels.indexOf(minutes);\r\n        if (index === -1) {\r\n          this.chartData.labels.push(minutes);\r\n          this.chartData.datasets.forEach((dataset, i) => {\r\n            dataset.data.push(values[i] || 0);\r\n          });\r\n        } else {\r\n          this.chartData.datasets.forEach((dataset, i) => {\r\n            dataset.data[index] = values[i] || 0;\r\n          });\r\n        }\r\n      }\r\n    }\r\n  });\r\n  </script>\r\n  \r\n  <style>\r\n  .popup {\r\n    position: fixed;\r\n    top: 20%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    background-color: white;\r\n    padding: 20px;\r\n    border: 1px solid black;\r\n    z-index: 1000;\r\n  }\r\n  \r\n  .popup button {\r\n    margin-right: 10px;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAsCE,SAASA,eAAc,QAAS,KAAI;AACpC,SAASC,IAAG,QAAS,aAAY;AACjC,SAASC,KAAI,IAAKC,OAAO,EAAEC,KAAK,EAAEC,OAAO,EAAEC,MAAM,EAAEC,WAAW,EAAEC,YAAY,EAAEC,aAAa,EAAEC,WAAU,QAAS,UAAS;;AAEzH;AACAP,OAAO,CAACQ,QAAQ,CAACP,KAAK,EAAEC,OAAO,EAAEC,MAAM,EAAEC,WAAW,EAAEC,YAAY,EAAEC,aAAa,EAAEC,WAAW;AAE9F,eAAeV,eAAe,CAAC;EAC7BY,UAAU,EAAE;IACVC,SAAS,EAAEZ;EACb,CAAC;EACDa,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,KAAK;MAChBC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE,IAAI;MACjBC,SAAS,EAAE;QACTC,MAAM,EAAE,EAAE;QAAE;QACZC,QAAQ,EAAE,CACR;UACEC,KAAK,EAAE,aAAa;UACpBT,IAAI,EAAE,EAAE;UACRU,WAAW,EAAE,MAAM;UACnBC,eAAe,EAAE,sBAAsB;UACvCC,IAAI,EAAE;QACR,CAAC,EACD;UACEH,KAAK,EAAE,aAAa;UACpBT,IAAI,EAAE,EAAE;UACRU,WAAW,EAAE,KAAK;UAClBC,eAAe,EAAE,sBAAsB;UACvCC,IAAI,EAAE;QACR,CAAC,EACD;UACEH,KAAK,EAAE,aAAa;UACpBT,IAAI,EAAE,EAAE;UACRU,WAAW,EAAE,KAAK;UAClBC,eAAe,EAAE,sBAAsB;UACvCC,IAAI,EAAE;QACR,CAAC,EACD;UACEH,KAAK,EAAE,aAAa;UACpBT,IAAI,EAAE,EAAE;UACRU,WAAW,EAAE,KAAK;UAClBC,eAAe,EAAE,sBAAsB;UACvCC,IAAI,EAAE;QACR;QACA;QAAA;MAEJ,CAAC;MACDC,YAAY,EAAE;QACZC,UAAU,EAAE,IAAI;QAChBC,MAAM,EAAE;UACNC,CAAC,EAAE;YACDC,KAAK,EAAE;cACLC,OAAO,EAAE,IAAI;cACbC,IAAI,EAAE;YACR;UACF,CAAC;UACDC,CAAC,EAAE;YACDH,KAAK,EAAE;cACLC,OAAO,EAAE,IAAI;cACbC,IAAI,EAAE;YACR,CAAC;YACDE,GAAG,EAAE,CAAC;YACNC,GAAG,EAAE;UACP;QACF;MACF;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,EAAC,GAAI,IAAIC,SAAS,CAAC,qBAAqB,CAAC;IAC/CD,EAAE,CAACE,SAAQ,GAAKC,KAAK,IAAK;MACxB,MAAM3B,IAAG,GAAI4B,IAAI,CAACC,KAAK,CAACF,KAAK,CAAC3B,IAAI,CAAC;MACnC,IAAI,CAAC,IAAI,CAACE,WAAW,CAAC4B,QAAQ,CAAC9B,IAAI,CAAC+B,EAAE,CAAC,EAAE;QACvC,IAAI,CAAC3B,SAAQ